# notam.php

# Environment
SetEnv APP_ENV production

# Database credentials
SetEnv DB_SERVER localhost
SetEnv DB_NAME xcsoar_notam
SetEnv DB_USER notam
SetEnv DB_PASS your_db_password

# FAA NMS API config
SetEnv FAA_API_BASE https://api-staging.cgifederal-aim.com/nmsapi
